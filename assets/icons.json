{{- $icons := slice -}}
{{- range sort (resources.Match "icons/**/*.svg") "Name" -}}
  {{- $match := findRESubmatch `icons/(.+)/(.+)\.svg` .Name -}}
  {{- $info := index $match 0 -}}
  {{- $icons = $icons | append (dict
    "vendor" (index $info 1)
    "name" (index $info 2)
    "url" .RelPermalink)
  -}}
{{- end -}}
{{- $icons | jsonify -}}